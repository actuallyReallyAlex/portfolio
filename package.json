{
  "name": "alexlee-dev-portfolio",
  "description": "Alex Lee's portfolio. Built with React.",
  "version": "2.1.0",
  "dependencies": {
    "grommet": "^2.7.1",
    "grommet-icons": "^4.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hook-form": "^3.13.1",
    "react-intersection-observer": "^8.23.0",
    "react-scripts": "3.0.1",
    "react-tilt": "^0.1.4",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coveralls": "jest --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/hooks/**/* --collectCoverageFrom=!src/setupTests.js --coverageReporters=text-lcov | coveralls"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "public",
    "src",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlee-dev/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/alexlee-dev/portfolio/issues"
  },
  "homepage": "https://alexlee.dev/",
  "main": "index.js",
  "keywords": [
    "portfolio"
  ],
  "author": "Alex Lee <alexlee-dev>",
  "license": "GPL-3.0",
  "devDependencies": {
    "coveralls": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  }
}
